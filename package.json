{
  "name": "kamu-platform",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write '**/*.{ts,js,html,spec.ts}'",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "gql-update-schema": "curl https://raw.githubusercontent.com/kamu-data/kamu-cli/master/resources/schema.gql > resources/schema.graphql",
    "gql-codegen": "graphql-codegen --config gql-codegen.yml && prettier --write src/app/api/kamu.graphql.interface.ts && sed -i 's/import { gql } from \"apollo-angular\"/import { gql } from \"@apollo\\/client\\/core\"/g' src/app/api/kamu.graphql.interface.ts",
    "prepare": "husky install",
    "install_pretty_quick": "npx husky set .husky/pre-commit \"npx pretty-quick --staged\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/elements": "^11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "11.2.14",
    "@angular/material": "~9.0.0-rc.0",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@angular/service-worker": "11.2.14",
    "@apollo/client": "3.0.0-beta.50",
    "@ng-bootstrap/ng-bootstrap": "6.0.0",
    "@popperjs/core": "^2.10.2",
    "@swimlane/ngx-graph": "8.0.0-rc.1",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "d3-scale": "^3.2.1",
    "graphql": "^14.7.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.31",
    "ngx-markdown": "^9.1.1",
    "ngx-monaco-editor": "^9.0.0",
    "npx": "^10.2.2",
    "rxjs": "~6.5.3",
    "subsink": "^1.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.11.8",
    "@graphql-codegen/typescript-apollo-angular": "^3.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "9.6.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "codelyzer": "^5.1.2",
    "eslint": "^8.22.0",
    "husky": "^8.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "4.0.8"
  }
}
